#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,a[100],num,c;
	printf("Enter the number of elements:\n");
	fflush(stdin);fflush(stdout);
	scanf("%d",&num);
	for(i=0;i<num;i++)
	{
		a[i]=11*(i+1);
		printf("%d\t",a[i]);
	}
	printf("Enter the elements to be searched:\n");
	fflush(stdin);fflush(stdout);
	scanf("%d",&c);
	for(i=0;i<num;i++)
	{
		if(a[i] == c)
		{
			printf("Number found at location = %d",i+1);
		}

	}
	return 0;
}
