#include "US_Sensor.h"

extern void (*US_state)();

//modules variables
unsigned int distance;

//generate random values
int randValues (int l,int r,int count)
{
	int i;
	for(i=0;i<count;i++)
	{
		int rand_num = (rand()%(r-l+1))+1;
		return rand_num;
	}
}


void US_init()
{
	//init US_Sensor & call  drivers
	printf("US-----init \n");
}

